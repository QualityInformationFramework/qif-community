            minOccurs="0">
            <xs:annotation>
              <xs:documentation>
                The optional CoordinateSystemId element is the QIF id of
                the coordinate system in which this characteristic is
                evaluated.
              </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

