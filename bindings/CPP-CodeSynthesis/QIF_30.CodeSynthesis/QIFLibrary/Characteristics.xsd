      <xs:extension base="WeldOneSideParametersExtendSizeType">
        <xs:sequence>
          <xs:element name="LengthOfEachWeld"
            type="LinearValueType"
            minOccurs="0">
            <xs:annotation>
              <xs:documentation>
                The optional LengthOfEachWeld element specifies the length
                of each weld.
              </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Pitch"
            type="LinearValueType"
            minOccurs="0">
            <xs:annotation>
              <xs:documentation>
                The optional Pitch element specifies the pitch of the weld.
              </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="WeldFilletUnequalLegLengthType">
    <xs:annotation>
      <xs:documentation>
        The WeldFilletUnequalLegLengthType describes unequal leg lengths of
        a fillet weld.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
