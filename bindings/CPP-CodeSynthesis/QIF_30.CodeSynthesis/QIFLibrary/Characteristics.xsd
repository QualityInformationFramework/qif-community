      <xs:enumeration value="OXYACETYLENE_WELDING"/>
      <xs:enumeration value="FRICTION_WELDING"/>
      <xs:enumeration value="FORGE_WELDING"/>
      <xs:enumeration value="THERMIT_WELDING"/>
      <xs:enumeration value="ELECTROSLAG_WELDING"/>
      <xs:enumeration value="STUD_ARC_WELDING"/>
