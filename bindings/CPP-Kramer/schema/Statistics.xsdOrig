<!-- TO PASS-FAIL CHARACTERISTICS -->

  <xs:element name="PassFailStatsValue"
    type="StatsWithReferenceBaseType"
    abstract="true">
    <xs:annotation>
      <xs:documentation>
        The PassFailStatsValue global element is a statistical value
        related to pass-fail characteristics. This is the head of a
        substitution group.
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="FailurePercentage"
    type="StatsMeasuredDecimalType"
    substitutionGroup="PassFailStatsValue">
    <xs:annotation>
      <xs:documentation>
        The FailurePercentage global element is the fraction of
        included observations that have failed expressed as a
        number between 0.0 and 1.0.
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="NumberFailures"
    type="StatsNonNegativeIntegerType"
    substitutionGroup="PassFailStatsValue">
    <xs:annotation>
      <xs:documentation>
        The NumberFailures global element is the number of
        included observations that have failed.
      </xs:documentation>
    </xs:annotation>
  </xs:element>

