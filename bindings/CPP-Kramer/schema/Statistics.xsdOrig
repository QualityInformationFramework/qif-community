<!--  BEGIN CHARACTERISTIC STATS LISTS TYPES -->

  <xs:complexType name="CharacteristicsStatsType">
    <xs:annotation>
      <xs:documentation>
        The CharacteristicsStatsType defines a list of characteristic
        statistical evaluations.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element ref="CharacteristicStats"
        maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>
            Each CharacteristicStats element gives information about a
            characteristic statistical evaluation.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="n"
      type="NaturalType"
      use="required">
      <xs:annotation>
        <xs:documentation>
          The required n attribute is the number of characteristic
          statistical evaluations in the list.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <xs:element name="CharacteristicsStats"
    type="CharacteristicsStatsType">
    <xs:annotation>
      <xs:documentation>
        The global CharacteristicsStats element represents a list of
        characteristic statistical evaluations.
      </xs:documentation>
    </xs:annotation>
  </xs:element>

<!--  BEGIN AVERAGE FEATURE TYPE -->

  <xs:complexType name="AverageFeatureType">
    <xs:annotation>
      <xs:documentation>
        The AverageFeatureType defines a feature measurement which is the
        average representation of a list of feature measurements.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element ref="FeatureMeasurement">
        <xs:annotation>
          <xs:documentation>
            The FeatureMeasurement element is the average feature.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="MeasuredIds"
        type="StatsArrayIdType">
        <xs:annotation>
          <xs:documentation>
            The MeasuredIds element contains a list of ids of the feature
            measurements used to calculate the average feature and an
            optional set of exclusions from the list.
          </xs:documentation>
        </xs:annotation>

        <xs:key name="MeasuredFeatureIdsKey">
          <xs:annotation>
            <xs:documentation>
              The MeasuredFeatureIdsKey collects all QIF ids of measured
              features being averaged.
            </xs:documentation>
          </xs:annotation>
          <xs:selector xpath="t:Ids/t:Id"/>
          <xs:field xpath="."/>
        </xs:key>

        <xs:keyref name="MeasuredFeatureIdExclusionKeyref"
          refer="MeasuredFeatureIdsKey">
          <xs:annotation>
            <xs:documentation>
              The MeasuredFeatureIdExclusionKeyref requires that every
              excluded Id be a measured feature Id in the Ids.
            </xs:documentation>
          </xs:annotation>
          <xs:selector xpath="t:Exclusions/t:Exclusion/t:Id"/>
          <xs:field xpath="."/>
        </xs:keyref>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:element name="AverageFeature"
    type="AverageFeatureType">
    <xs:annotation>
      <xs:documentation>
        The global AverageFeature element represents a single average
        feature.
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:complexType name="AverageFeaturesType">
    <xs:annotation>
      <xs:documentation>
        The AverageFeaturesType defines a list of average features.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element ref="AverageFeature"
        maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>
            Each AverageFeature element gives information about an
            average feature.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="n"
      type="NaturalType"
      use="required">
      <xs:annotation>
        <xs:documentation>
          The required n attribute is the number of average features in the
          list.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <xs:element name="AverageFeatures"
    type="AverageFeaturesType">
    <xs:annotation>
      <xs:documentation>
        The global AverageFeatures element is a list of an average
        features.
      </xs:documentation>
    </xs:annotation>
  </xs:element>

<!--  BEGIN SUMMARY STATISTICS TYPE -->

