  <xs:complexType name="OtherCurveFeatureItemType">
    <xs:annotation>
      <xs:documentation>
        The OtherCurveFeatureItemType defines an individual other curve
        feature.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="CurveFeatureItemBaseType">
        <xs:sequence>
          <xs:element name="DeterminationMode"
            type="OtherCurveMeasurementDeterminationType">
            <xs:annotation>
              <xs:documentation>
